pyautogui.press("enter")
pyautogui.sleep(3)
pyautogui.press("tab")
pyautogui.sleep(3)
pyautogui.press("tab")
pyautogui.sleep(3)
pyautogui.write("Computador\HKEY_LOCAL_MACHINE\SYSTEM\ControlSet001\Control")
pyautogui.sleep(6)
pyautogui.press("enter")
